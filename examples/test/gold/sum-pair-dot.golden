digraph sum_pair {
  margin=0
  compound=true
  rankdir=LR
  node [shape=Mrecord]
  edge [fontsize=8,fontcolor=indigo]
  bgcolor=transparent
  nslimit=20
  c0 [label="{In|{<Out0>|<Out1>}}"]
  c1 [label="{{<In0>|<In1>}|+|{<Out0>}}"]
  c2 [label="{{<In0>}|Out}"]
  c0:Out0 -> c1:In0 [label="Int"]
  c0:Out1 -> c1:In1 [label="Int"]
  c1:Out0 -> c2:In0 [label="Int"]
}
